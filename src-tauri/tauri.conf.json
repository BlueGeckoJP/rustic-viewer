{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "rustic-viewer",
  "version": "0.1.0",
  "identifier": "me.bluegecko.rustic-viewer",
  "build": {
    "beforeDevCommand": "pnpm wasm:build && pnpm test && pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "rustic-viewer",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null,
      "headers": {
        "Cross-Origin-Opener-Policy": "same-origin",
        "Cross-Origin-Embedder-Policy": "require-corp"
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
